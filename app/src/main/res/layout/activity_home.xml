        android:padding="0dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="70dp" />

    <Button
        android:id="@+id/btn_register_bin"
        android:layout_width="149dp"
        android:layout_height="176dp"
